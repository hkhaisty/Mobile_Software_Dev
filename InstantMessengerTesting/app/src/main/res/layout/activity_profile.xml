<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background"
        tools:context=".ProfileActivity">

    <TextView
            android:text="Hello, Harriet"
            android:fontFamily="@font/northen"
            android:textColor="@android:color/white"
            android:textSize="35dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="52dp"
            android:layout_marginRight="52dp"
            app:layout_constraintStart_toEndOf="@+id/profileimage"
            android:layout_marginLeft="11dp"
            android:layout_marginStart="11dp"
            android:layout_marginTop="279dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintBottom_toTopOf="@+id/textView4"/>
    <TextView
            android:text="hhaisty@students.kennesaw.edu"
            android:fontFamily="@font/northen"
            android:textColor="@android:color/white"
            android:textSize="15.2dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="164dp"
            android:layout_marginStart="164dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="23dp"
            android:layout_marginRight="23dp"
            android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginBottom="28dp"
            app:layout_constraintBottom_toTopOf="@+id/profileimage"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"/>
    <ImageView
            android:layout_width="177dp"
            android:layout_height="181dp"
            android:src="@drawable/roundprofile"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="40dp"
            android:scaleType="centerInside"
            android:id="@+id/profileimage"
            android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.058"
    />
    <ImageView
            android:layout_width="131dp"
            android:layout_height="143dp"
            android:src="@drawable/qrcodegeneratorprofile"
            android:id="@+id/qrcodegeneratorbutton"
            android:layout_marginTop="72dp"
            app:layout_constraintTop_toBottomOf="@+id/profileimage"
            app:layout_constraintEnd_toStartOf="@+id/searchqrcodebutton" android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginBottom="21dp"
            app:layout_constraintBottom_toTopOf="@+id/chatimagebutton"/>

    <ImageView
            android:layout_width="131dp"
            android:layout_height="142dp"
            android:src="@drawable/chatbutton"
            android:id="@+id/chatimagebutton"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="132dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/qrcodegeneratorbutton"
            app:layout_constraintEnd_toStartOf="@+id/friendsimagebutton" android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"/>
    <ImageView
            android:layout_width="131dp"
            android:layout_height="143dp"
            android:src="@drawable/qrcodebuttonsearchprofile"
            android:layout_marginLeft="5dp"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toEndOf="@+id/qrcodegeneratorbutton"
            android:id="@+id/searchqrcodebutton"
            android:layout_marginTop="72dp" app:layout_constraintTop_toBottomOf="@+id/profileimage"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toTopOf="@+id/friendsimagebutton"/>
    <ImageView
            android:layout_width="131dp"
            android:layout_height="143dp"
            android:src="@drawable/friendsbutton"
            android:id="@+id/friendsimagebutton"
            android:layout_marginBottom="132dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/searchqrcodebutton" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toEndOf="@+id/chatimagebutton" android:layout_marginLeft="5dp"
            android:layout_marginStart="5dp"/>
</android.support.constraint.ConstraintLayout>